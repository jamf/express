WHITESPACE = _{ " " | "\t" | "\n" | "\r" }

symbol_first_char = _{ ASCII_ALPHA }
symbol_char = _{ symbol_first_char | ASCII_DIGIT | DASH_PUNCTUATION }
symbol = @{ symbol_first_char ~ symbol_char* }

raw_text = ${ ( !"{{" ~ ANY )+ }

variable = !{ "{{" ~ symbol ~ "}}" }

element = { raw_text |
            variable }

elements = @{ element* }

template = _{ SOI ~ elements ~ EOI }